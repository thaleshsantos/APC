def banner(n):
    n = int(n)
    resto = n%2
    if (resto == 0 and n > 0):
        print('| | | | | | | | | |')
    elif (resto == 1 and n > 0):
        print('- - - - - - - - - -')
    elif (resto == 0 and n < 0):
        print ('. . . . . . . . . .')
    else:
        print('= = = = = = = = = =')
